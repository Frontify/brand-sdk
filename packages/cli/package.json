{
    "name": "@frontify/frontify-cli",
    "author": "Frontify Developers <developers@frontify.com>",
    "version": "5.3.6",
    "bin": {
        "frontify-cli": "dist/index.mjs"
    },
    "engines": {
        "node": ">=16"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Frontify/brand-sdk",
        "directory": "packages/app-bridge"
    },
    "files": [
        "dist",
        "templates"
    ],
    "type": "module",
    "scripts": {
        "build": "vite build",
        "format": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "start": "npm run build && node dist/index.mjs",
        "test": "vitest run --silent",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@fastify/cors": "8.2.0",
        "@vitejs/plugin-react": "3.0.1",
        "archiver": "5.3.1",
        "cac": "^6.7.14",
        "conf": "11.0.1",
        "fast-glob": "3.2.12",
        "fastify": "4.12.0",
        "glob-to-regexp": "^0.4.1",
        "node-fetch": "3.3.0",
        "open": "8.4.0",
        "picocolors": "1.0.0",
        "prompts": "2.4.2",
        "vite": "4.0.4",
        "vite-plugin-externals": "0.5.1"
    },
    "devDependencies": {
        "@frontify/eslint-config-typescript": "0.15.5",
        "@types/glob-to-regexp": "^0.4.1",
        "@types/mock-fs": "4.13.1",
        "@types/node": "18.11.18",
        "@types/prompts": "^2.4.2",
        "@types/ws": "8.5.4",
        "@vitest/coverage-c8": "0.28.1",
        "@vitest/ui": "^0.28.1",
        "eslint": "8.32.0",
        "eslint-plugin-notice": "^0.9.10",
        "mock-fs": "5.2.0",
        "nock": "13.3.0",
        "prettier": "2.8.3",
        "ts-node": "10.9.1",
        "typescript": "4.9.4",
        "vitest": "0.28.1"
    }
}

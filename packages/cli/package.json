{
    "name": "@frontify/frontify-cli",
    "type": "module",
    "version": "5.8.7",
    "author": "Frontify Developers <developers@frontify.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/Frontify/brand-sdk",
        "directory": "packages/cli"
    },
    "bin": {
        "frontify-cli": "dist/index.mjs"
    },
    "files": [
        "dist",
        "templates"
    ],
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build": "vite build",
        "format": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "start": "npm run build && node dist/index.mjs",
        "test": "vitest run --silent",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@fastify/cors": "^9.0.1",
        "@vitejs/plugin-react": "^4.3.4",
        "archiver": "^7.0.1",
        "cac": "^6.7.14",
        "conf": "^13.1.0",
        "esbuild": "^0.21.5",
        "fast-glob": "^3.3.3",
        "fastify": "^4.29.0",
        "glob-to-regexp": "^0.4.1",
        "node-fetch": "^3.3.2",
        "open": "^10.1.0",
        "picocolors": "^1.1.1",
        "prompts": "^2.4.2",
        "vite": "^5.4.18",
        "vite-plugin-externals": "^0.6.2",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@frontify/eslint-config-basic": "^0.20.6",
        "@types/glob-to-regexp": "^0.4.4",
        "@types/mock-fs": "^4.13.4",
        "@types/node": "^22.14.0",
        "@types/prompts": "^2.4.9",
        "@types/ws": "8.18.1",
        "@vitest/coverage-v8": "3.1.1",
        "@vitest/ui": "^3.1.1",
        "eslint": "^8.57.1",
        "eslint-plugin-notice": "^1.0.0",
        "mock-fs": "^5.5.0",
        "nock": "^13.5.6",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "vitest": "^3.1.1"
    }
}

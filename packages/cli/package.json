{
    "name": "@frontify/frontify-cli",
    "type": "module",
    "version": "5.7.4",
    "author": "Frontify Developers <developers@frontify.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/Frontify/brand-sdk",
        "directory": "packages/cli"
    },
    "bin": {
        "frontify-cli": "dist/index.mjs"
    },
    "files": [
        "dist",
        "templates"
    ],
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build": "vite build",
        "format": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "start": "npm run build && node dist/index.mjs",
        "test": "vitest run --silent",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@fastify/cors": "^9.0.1",
        "@vitejs/plugin-react": "^4.2.1",
        "archiver": "^7.0.1",
        "cac": "^6.7.14",
        "conf": "^12.0.0",
        "esbuild": "^0.21.5",
        "fast-glob": "^3.3.2",
        "fastify": "^4.26.2",
        "glob-to-regexp": "^0.4.1",
        "node-fetch": "^3.3.2",
        "open": "^10.1.0",
        "picocolors": "^1.0.0",
        "prompts": "^2.4.2",
        "vite": "^5.2.10",
        "vite-plugin-externals": "^0.6.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@frontify/eslint-config-basic": "^0.20.5",
        "@types/glob-to-regexp": "^0.4.4",
        "@types/mock-fs": "^4.13.4",
        "@types/node": "^18.19.31",
        "@types/prompts": "^2.4.9",
        "@types/ws": "8.5.10",
        "@vitest/coverage-v8": "1.4.0",
        "@vitest/ui": "^1.4.0",
        "eslint": "^8.57.0",
        "eslint-plugin-notice": "^0.9.10",
        "mock-fs": "^5.2.0",
        "nock": "^13.5.4",
        "prettier": "^3.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.4",
        "vitest": "^1.4.0"
    }
}

{
    "name": "@frontify/frontify-cli",
    "version": "5.1.2",
    "bin": {
        "frontify-cli": "dist/index.mjs"
    },
    "engines": {
        "node": ">=16"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "scripts": {
        "build": "vite build",
        "format": "prettier --write src",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "prepare": "is-ci || husky install",
        "start": "npm run build && node dist/index.mjs",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "typecheck": "tsc --noEmit"
    },
    "author": "Frontify Developers <developers@frontify.com>",
    "dependencies": {
        "@fastify/cors": "8.1.1",
        "@vitejs/plugin-react": "2.2.0",
        "archiver": "5.3.1",
        "cac": "^6.7.14",
        "conf": "10.2.0",
        "fast-glob": "3.2.12",
        "fastify": "4.9.2",
        "node-fetch": "^3.2.10",
        "open": "8.4.0",
        "picocolors": "^1.0.0",
        "prompts": "^2.4.2",
        "vite": "3.2.2",
        "vite-plugin-externals": "0.5.1"
    },
    "devDependencies": {
        "@frontify/eslint-config-typescript": "0.15.2",
        "@types/mock-fs": "4.13.1",
        "@types/node": "18.11.8",
        "@types/prompts": "^2.4.1",
        "@types/ws": "8.5.3",
        "@vitest/coverage-c8": "0.24.4",
        "@vitest/ui": "^0.24.4",
        "eslint": "8.26.0",
        "husky": "8.0.1",
        "is-ci": "3.0.1",
        "mock-fs": "5.2.0",
        "nock": "13.2.9",
        "prettier": "2.7.1",
        "ts-node": "10.9.1",
        "typescript": "4.8.4",
        "vitest": "0.24.4"
    }
}

{
    "name": "@frontify/frontify-cli",
    "version": "1.1.1",
    "bin": {
        "frontify-cli": "dist/main.js"
    },
    "engines": {
        "node": ">=15"
    },
    "files": [
        "dist",
        "types",
        "shims.js"
    ],
    "types": "index.d.ts",
    "scripts": {
        "build": "cross-env NODE_ENV=production npm run clean && npm run build:ts",
        "build:ts": "esbuild src/main.ts --platform=node --bundle --external:esbuild '--define:process.env.NODE_ENV=\"production\"' --external:fsevents --outfile=dist/main.js",
        "clean": "rimraf dist",
        "format": "prettier --write 'src/**/*.ts'",
        "lint": "eslint 'src/**/*.ts'",
        "lint:fix": "eslint '*/**/*.ts' --fix",
        "prepare": "is-ci || husky install",
        "prepublishOnly": "npm run build",
        "start": "npm run build && node dist/main.js",
        "test": "jest"
    },
    "author": "Frontify Developers <developers@frontify.com>",
    "dependencies": {
        "esbuild": "^0.12.14"
    },
    "devDependencies": {
        "@frontify/eslint-config-typescript": "^0.7.0",
        "@types/archiver": "^5.1.0",
        "@types/express": "^4.17.12",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.171",
        "@types/minimist": "^1.2.1",
        "@types/mock-fs": "^4.13.0",
        "@types/node": "^15.14.0",
        "@types/node-fetch": "^2.5.10",
        "@types/ws": "^7.4.5",
        "archiver": "^5.3.0",
        "chalk": "^4.1.1",
        "chokidar": "^3.5.2",
        "conf": "^10.0.1",
        "cross-env": "^7.0.3",
        "esbuild-css-modules-plugin": "^1.0.6",
        "eslint": "^7.29.0",
        "fast-glob": "^3.2.6",
        "fastify": "^3.18.1",
        "fastify-cors": "^6.0.1",
        "fastify-static": "^4.2.2",
        "fastify-websocket": "^3.2.0",
        "husky": "^7.0.1",
        "is-ci": "^3.0.0",
        "jest": "^27.0.6",
        "lodash": "^4.17.21",
        "minimist": "^1.2.5",
        "mock-fs": "^5.0.0",
        "nock": "^13.1.0",
        "node-fetch": "^2.6.1",
        "open": "^8.2.1",
        "prettier": "^2.3.2",
        "react": "^17.0.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.5"
    }
}

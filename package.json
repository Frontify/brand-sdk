{
    "name": "@frontify/frontify-cli",
    "version": "5.0.0",
    "bin": {
        "frontify-cli": "dist/index.js"
    },
    "engines": {
        "node": ">=16"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "format": "prettier --write 'src/**/*.ts'",
        "lint": "eslint 'src/**/*.ts'",
        "lint:fix": "eslint '*/**/*.ts' --fix",
        "prepare": "is-ci || husky install",
        "prepublishOnly": "npm run build",
        "start": "npm run build && node dist/index.js",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "author": "Frontify Developers <developers@frontify.com>",
    "dependencies": {
        "@fastify/cors": "^8.0.0",
        "@fastify/static": "^6.4.0",
        "@fastify/websocket": "^6.0.1",
        "fastify": "4.2.1",
        "minimist": "1.2.6",
        "minimist-options": "4.1.0",
        "vite": "2.9.14"
    },
    "devDependencies": {
        "@babel/core": "^7.18.9",
        "@frontify/eslint-config-typescript": "0.15.2",
        "@types/archiver": "5.3.1",
        "@types/express": "4.17.13",
        "@types/lodash-es": "4.17.6",
        "@types/minimist": "1.2.2",
        "@types/mock-fs": "4.13.1",
        "@types/node": "^17.0.25",
        "@types/ws": "8.5.3",
        "archiver": "5.3.1",
        "c8": "^7.12.0",
        "colors": "^1.4.0",
        "conf": "10.1.2",
        "eslint": "8.20.0",
        "fast-glob": "3.2.11",
        "husky": "8.0.1",
        "is-ci": "3.0.1",
        "lodash-es": "4.17.21",
        "mock-fs": "5.1.2",
        "nock": "13.2.9",
        "node-fetch": "3.2.9",
        "open": "8.4.0",
        "postcss": "^8.4.14",
        "prettier": "2.7.1",
        "react": "17.0.2",
        "rollup": "^2.77.0",
        "rollup-plugin-copy": "^3.4.0",
        "ts-node": "10.9.1",
        "typescript": "4.7.4",
        "vitest": "0.18.1"
    }
}

{
    "name": "@frontify/frontify-cli",
    "version": "5.1.1",
    "bin": {
        "frontify-cli": "dist/index.js"
    },
    "engines": {
        "node": ">=16"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "format": "prettier --write 'src/**/*.ts'",
        "lint": "eslint 'src/**/*.ts'",
        "lint:fix": "eslint '*/**/*.ts' --fix",
        "prepare": "is-ci || husky install",
        "prepublishOnly": "npm run build",
        "start": "npm run build && node dist/index.js",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "author": "Frontify Developers <developers@frontify.com>",
    "dependencies": {
        "@fastify/cors": "^8.0.0",
        "@vitejs/plugin-react": "^2.0.0",
        "fastify": "4.5.2",
        "minimist": "1.2.6",
        "minimist-options": "4.1.0",
        "vite": "3.0.4",
        "vite-plugin-externals": "^0.5.1"
    },
    "devDependencies": {
        "@frontify/eslint-config-typescript": "0.15.2",
        "@types/minimist": "1.2.2",
        "@types/mock-fs": "4.13.1",
        "@types/node": "^17.0.45",
        "@types/ws": "8.5.3",
        "archiver": "5.3.1",
        "c8": "7.12.0",
        "colors": "^1.4.0",
        "conf": "10.2.0",
        "eslint": "8.21.0",
        "fast-glob": "3.2.11",
        "husky": "8.0.1",
        "is-ci": "3.0.1",
        "mock-fs": "5.1.4",
        "nock": "13.2.9",
        "node-fetch": "3.2.10",
        "open": "8.4.0",
        "prettier": "2.7.1",
        "ts-node": "10.9.1",
        "typescript": "4.7.4",
        "vitest": "0.22.1"
    }
}
